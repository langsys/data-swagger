{
    "name": "langsys/swagger-auto-generator",
    "description": "Automatic Swagger and DTO Generator Docs Generator",
    "type": "library",
    "license": "MIT",
    "require": {
        "darkaonline/l5-swagger": "^8.4",
        "spatie/laravel-data": "^4.7"
    },
    "authors": [
        {
          "role": "Developer",
          "name": "Humberto Cuadra",
          "email": "hcuadra@flexmarkintl.com"
        },

        {
            "role": "Developer",
            "name": "Jarliev",
            "email": "jarliev2@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Langsys\\SwaggerAutoGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Langsys\\SwaggerAutoGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Langsys\\SwaggerAutoGenerator\\SwaggerAutoGeneratorServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "3.x-dev",
        "orchestra/testbench": "9.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
